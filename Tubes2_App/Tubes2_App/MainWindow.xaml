<Window x:Class="Tubes2_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tubes2_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="800">
    <Grid Margin="0,0,-0.4,-285" Name="mainGrid" Background="{DynamicResource {x:Static SystemColors.ControlLightLightBrushKey}}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="People You May Know" VerticalAlignment="Top" Height="35" Width="252" TextAlignment="Center" FontSize="22" FontWeight="Bold"/>
        <TextBlock HorizontalAlignment="Left" Height="20" Margin="183,77,0,0" TextWrapping="Wrap" Text="Graph File :" VerticalAlignment="Top" Width="65" RenderTransformOrigin="1.557,0.5"/>
        <TextBlock HorizontalAlignment="Left" Height="20" Margin="183,102,0,0" TextWrapping="Wrap" Text="Algorithm :" VerticalAlignment="Top" Width="65"/>
        <TextBlock HorizontalAlignment="Left" Height="20" Margin="185,370,0,0" TextWrapping="Wrap" Text="Choose Account :" VerticalAlignment="Top" Width="99"/>
        <TextBlock HorizontalAlignment="Left" Height="20" Margin="185,395,0,0" TextWrapping="Wrap" Text="Explore Friends With :" VerticalAlignment="Top" Width="120"/>
        <Button Content="Submit" HorizontalAlignment="Left" Height="33" Margin="185,420,0,0" VerticalAlignment="Top" Width="65" Click="Button_Click"/>
        <Button Content="Browse" HorizontalAlignment="Left" Height="20" Margin="253,77,0,0" VerticalAlignment="Top" Width="50" Click="Browse_File_Button" Name="BrowseButton"/>
        <RadioButton Content="DFS" HorizontalAlignment="Left" Height="20" Margin="253,102,0,0" VerticalAlignment="Top" Width="50" Checked="RadioButton_Checked"/>
        <RadioButton Content="BFS" HorizontalAlignment="Left" Height="20" Margin="303,103,0,0" VerticalAlignment="Top" Width="43"/>
        <ComboBox HorizontalAlignment="Left" Height="20" Margin="315,370,0,0" VerticalAlignment="Top" Width="76" Name="Choose_Account_ComboBox" SelectionChanged="Choose_Account_ComboBox_SelectionChanged"/>
        <ComboBox HorizontalAlignment="Left" Height="20" Margin="315,395,0,0" VerticalAlignment="Top" Width="76" Name="Explore_ComboBox" SelectionChanged="Explore_ComboBox_SelectionChanged"/>
        <Canvas HorizontalAlignment="Left" Height="229" Margin="183,137,0,0" VerticalAlignment="Top" Width="437" Name="graphCanvas">
        </Canvas>
        <Canvas HorizontalAlignment="Left" Height="237" Margin="183,472,0,0" VerticalAlignment="Top" Width="437" Name="friendCanvas"/>
    </Grid>
</Window>
